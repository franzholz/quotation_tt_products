{
  "name": "jambagecom/quotation-tt-products",
  "type": "typo3-cms-extension",
  "description": "Quotation for Shop System, Excel file export",
  "keywords": [
    "TYPO3",
    "extension",
    "parser",
    "cache",
    "stylesheet"
  ],
  "support": {
    "issues": "https://github.com/franzholz/quotation_tt_products/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^7.2",
    "typo3/cms-core": "^8.7 || ^9.5",
    "phpoffice/phpspreadsheet": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\QuotationTtProducts\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/quotation-tt-products": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "quotation_tt_products"
    }
  }
}
